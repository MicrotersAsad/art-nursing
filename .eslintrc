{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@next/next/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "no-debugger": "off",
    "no-restricted-globals": "off",
    "no-shadow": "off",
    "consistent-return": "off",
    "array-callback-return": "off",
    "default-case": "off",
    "dot-notation": "off",
    "eqeqeq": "off",
    "no-alert": "off",
    "no-else-return": "off",
    "no-empty-function": "off",
    "no-empty-pattern": "off",
    "no-eval": "off",
    "no-extend-native": "off",
    "no-extra-bind": "off",
    "no-extra-label": "off",
    "no-fallthrough": "off",
    "no-floating-decimal": "off",
    "no-global-assign": "off",
    "no-implicit-coercion": "off",
    "no-implied-eval": "off",
    "no-iterator": "off",
    "no-label-var": "off",
    "no-labels": "off",
    "no-lone-blocks": "off",
    "no-loop-func": "off",
    "no-magic-numbers": "off",
    "no-multi-str": "off",
    "no-new": "off",
    "no-new-func": "off",
    "no-new-wrappers": "off",
    "no-octal-escape": "off",
    "no-param-reassign": "off",
    "no-proto": "off",
    "no-redeclare": "off",
    "no-return-assign": "off",
    "no-script-url": "off",
    "no-self-assign": "off",
    "no-self-compare": "off",
    "no-sequences": "off",
    "no-throw-literal": "off",
    "no-unused-expressions": "off",
    "no-useless-call": "off",
    "no-useless-concat": "off",
    "no-useless-escape": "off",
    "no-void": "off",
    "no-warning-comments": "off",
    "no-with": "off",
    "vars-on-top": "off",
    "wrap-iife": "off",
    "yoda": "off",
    "strict": "off",
    "init-declarations": "off",
    "no-catch-shadow": "off",
    "no-delete-var": "off",
    "no-shadow-restricted-names": "off",
    "no-undef-init": "off",
    "no-undefined": "off",
    "no-use-before-define": "off",
     "react/display-name": "off",
       "react/no-unknown-property": "off"

  }
}
